{
    "name":"slym758/filament-mobile-table",
    "description": "Mobile responsive card view for Filament tables with featured fields, layouts, and colors",
    "keywords": ["filament", "laravel", "mobile", "responsive", "cards", "table"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Süleyman Ardıç",
            "email": "slym758@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "filament/filament": "^3.0|^4.0",
        "spatie/laravel-package-tools": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "MobileCards\\FilamentMobileTable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MobileCards\\FilamentMobileTable\\FilamentMobileTableServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

